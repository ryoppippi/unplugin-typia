{
  "name": "vite-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun -b vite",
    "build": "bun -b tsc && bun -b vite build",
    "preview": "bun -b vite preview",
    "deploy": "bun run build && npx wrangler pages deploy",
    "prepare": "ts-patch install && typia patch"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.22",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "typescript": "^5.4.5",
    "typia": "^6.0.6",
    "vite": "^5.2.0"
  }
}
