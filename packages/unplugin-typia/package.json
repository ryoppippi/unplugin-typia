{
	"name": "@ryoppippi/unplugin-typia",
	"type": "module",
	"version": "2.0.3",
	"private": true,
	"description": "unplugin for typia",
	"author": "ryoppippi",
	"license": "MIT",
	"repository": "https://github.com/ryoppippi/unplugin-typia.git",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"bun": "./src/index.ts",
			"default": "./dist/index.js"
		},
		"./api": {
			"types": "./dist/api.d.ts",
			"bun": "./src/api.ts",
			"default": "./dist/api.js"
		},
		"./vite": {
			"types": "./dist/vite.d.ts",
			"bun": "./src/vite.ts",
			"default": "./dist/vite.js"
		},
		"./webpack": {
			"types": "./dist/webpack.d.ts",
			"bun": "./src/webpack.ts",
			"default": "./dist/webpack.js"
		},
		"./rollup": {
			"types": "./dist/rollup.d.ts",
			"bun": "./src/rollup.ts",
			"default": "./dist/rollup.js"
		},
		"./esbuild": {
			"types": "./dist/esbuild.d.ts",
			"bun": "./src/esbuild.ts",
			"default": "./dist/esbuild.js"
		},
		"./next": {
			"types": "./dist/next.d.ts",
			"bun": "./src/next.ts",
			"default": "./dist/next.js"
		},
		"./rolldown": {
			"types": "./dist/rolldown.d.ts",
			"bun": "./src/rolldown.ts",
			"default": "./dist/rolldown.js"
		},
		"./farm": {
			"types": "./dist/farm.d.ts",
			"bun": "./src/farm.ts",
			"default": "./dist/farm.js"
		},
		"./rspack": {
			"types": "./dist/rspack.d.ts",
			"bun": "./src/rspack.ts",
			"default": "./dist/rspack.js"
		},
		"./bun": "./src/bun.ts",
		"./package.json": "./package.json"
	},
	"files": [
		"LICENSE",
		"README.md",
		"dist",
		"package.json",
		"src"
	],
	"scripts": {
		"build": "tsup",
		"lint": "nr check && eslint --cache .",
		"format": "eslint --cache --fix .",
		"test": "vitest --typecheck --testTimeout 150000",
		"check": "nr tsc --noEmit",
		"release": "nr test run && nr lint && nr build && nlx publint && bumpp --ignore-scripts",
		"prepublishOnly": "bun run build"
	},
	"peerDependencies": {
		"svelte": "^5.0.0",
		"typescript": ">=4.8.0 <5.9.0",
		"typia": ">=8.0.0 <9.0.0",
		"vite": ">=3.0.0"
	},
	"peerDependenciesMeta": {
		"svelte": {
			"optional": true
		},
		"vite": {
			"optional": true
		}
	},
	"dependencies": {
		"@rollup/pluginutils": "^5.1.4",
		"consola": "^3.4.0",
		"defu": "^6.1.4",
		"diff-match-patch-es": "^1.0.1",
		"find-cache-dir": "^5.0.0",
		"magic-string": "^0.30.17",
		"pathe": "^2.0.3",
		"pkg-types": "^2.1.0",
		"type-fest": "^4.36.0",
		"unplugin": "^2.2.0"
	},
	"devDependencies": {
		"@antfu/ni": "^23.3.1",
		"@rslib/core": "^0.5.4",
		"@ryoppippi/eslint-config": "npm:@jsr/ryoppippi__eslint-config@^0.0.23",
		"@types/bun": "^1.2.4",
		"@types/node": "^20.17.24",
		"@vue-macros/test-utils": "^1.7.1",
		"bumpp": "^10.0.3",
		"dax-sh": "^0.42.0",
		"esbuild": "^0.25.0",
		"eslint": "~9.21.0",
		"eslint-plugin-format": "^1.0.1",
		"rollup": "^4.34.9",
		"ts-patch": "^3.3.0",
		"tsup": "^8.4.0",
		"typescript": "~5.8.2",
		"typia": "^8.0.0",
		"unplugin-unused": "^0.4.3",
		"vite": "^6.2.0",
		"vitest": "^3.0.7"
	},
	"access": "public"
}
