{
	"name": "@ryoppippi/unplugin-typia",
	"type": "module",
	"version": "2.6.2",
	"private": true,
	"description": "unplugin for typia",
	"author": "ryoppippi",
	"license": "MIT",
	"funding": "https://github.com/ryoppippi/unplugin-typia?sponsor=1",
	"repository": "ryoppippi/unplugin-typia",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"bun": "./src/index.ts",
			"default": "./dist/index.js"
		},
		"./api": {
			"types": "./dist/api.d.ts",
			"bun": "./src/api.ts",
			"default": "./dist/api.js"
		},
		"./vite": {
			"types": "./dist/vite.d.ts",
			"bun": "./src/vite.ts",
			"default": "./dist/vite.js"
		},
		"./webpack": {
			"types": "./dist/webpack.d.ts",
			"bun": "./src/webpack.ts",
			"default": "./dist/webpack.js"
		},
		"./rollup": {
			"types": "./dist/rollup.d.ts",
			"bun": "./src/rollup.ts",
			"default": "./dist/rollup.js"
		},
		"./esbuild": {
			"types": "./dist/esbuild.d.ts",
			"bun": "./src/esbuild.ts",
			"default": "./dist/esbuild.js"
		},
		"./next": {
			"types": "./dist/next.d.ts",
			"bun": "./src/next.ts",
			"default": "./dist/next.js"
		},
		"./rolldown": {
			"types": "./dist/rolldown.d.ts",
			"bun": "./src/rolldown.ts",
			"default": "./dist/rolldown.js"
		},
		"./farm": {
			"types": "./dist/farm.d.ts",
			"bun": "./src/farm.ts",
			"default": "./dist/farm.js"
		},
		"./rspack": {
			"types": "./dist/rspack.d.ts",
			"bun": "./src/rspack.ts",
			"default": "./dist/rspack.js"
		},
		"./bun": "./src/bun.ts",
		"./package.json": "./package.json"
	},
	"files": [
		"LICENSE",
		"README.md",
		"dist",
		"package.json",
		"src"
	],
	"scripts": {
		"build": "tsdown",
		"lint": "run-p lint:*",
		"lint:eslint": "eslint --cache .",
		"lint:ts": "tsc --noEmit",
		"format": "eslint --cache --fix .",
		"test": "vitest",
		"prerelease": "run-p lint build 'test run'",
		"release": "bumpp",
		"prepack": "bun run build && nlx clean-pkg-json"
	},
	"peerDependencies": {
		"svelte": "^5.28.2",
		"typescript": ">=4.8.0 <5.9.0",
		"typia": ">=9.3.0",
		"vite": ">=3.0.0"
	},
	"peerDependenciesMeta": {
		"svelte": {
			"optional": true
		},
		"vite": {
			"optional": true
		}
	},
	"dependencies": {
		"@rollup/pluginutils": "^5.1.4",
		"consola": "^3.4.2",
		"defu": "^6.1.4",
		"diff-match-patch-es": "^1.0.1",
		"find-cache-dir": "^5.0.0",
		"magic-string": "^0.30.17",
		"pathe": "^2.0.3",
		"pkg-types": "^2.1.0",
		"type-fest": "^4.37.0",
		"unplugin": "^2.2.2"
	},
	"devDependencies": {
		"@antfu/ni": "^24.3.0",
		"@oxc-project/runtime": "^0.69.0",
		"@ryoppippi/eslint-config": "npm:@jsr/ryoppippi__eslint-config@^0.0.23",
		"@types/bun": "^1.2.12",
		"@types/node": "^20.17.46",
		"@vue-macros/test-utils": "^1.7.1",
		"bumpp": "^10.1.0",
		"dax-sh": "^0.42.0",
		"esbuild": "^0.25.4",
		"eslint": "~9.23.0",
		"eslint-plugin-format": "^1.0.1",
		"npm-run-all2": "^8.0.1",
		"publint": "^0.3.12",
		"rollup": "^4.36.0",
		"ts-patch": "^3.3.0",
		"tsdown": "^0.11.5",
		"typescript": "~5.8.3",
		"typia": "^9.3.0",
		"unplugin-unused": "^0.5.0",
		"vite": "^6.2.2",
		"vitest": "^3.0.9"
	},
	"access": "public"
}
