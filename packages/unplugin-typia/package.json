{
	"name": "@ryoppippi/unplugin-typia",
	"type": "module",
	"version": "0.6.16",
	"description": "unplugin for typia",
	"author": "ryoppippi",
	"license": "MIT",
	"keywords": [
		"unplugin",
		"vite",
		"webpack",
		"rollup",
		"transform"
	],
	"exports": {
		".": "./src/index.ts",
		"./api": "./src/api.ts",
		"./vite": "./src/vite.ts",
		"./webpack": "./src/webpack.ts",
		"./rollup": "./src/rollup.ts",
		"./esbuild": "./src/esbuild.ts",
		"./next": "./src/next.ts",
		"./rolldown": "./src/rolldown.ts",
		"./farm": "./src/farm.ts",
		"./rspack": "./src/rspack.ts",
		"./bun": "./src/bun.ts",
		"./package.json": "./package.json"
	},
	"files": [
		"LICENSE",
		"README.md",
		"jsr.json",
		"package.json",
		"src"
	],
	"scripts": {
		"lint": "nr check && eslint --cache .",
		"format": "eslint --cache --fix .",
		"test": "nr lint && bun test ./test/*.ts",
		"check": "nr tsc --noEmit",
		"release": "nr test && bumpp",
		"publish": "nr publish:prepare && nr test && nlx jsr publish --allow-dirty",
		"publish:prepare": "bun run ./scripts/gen_jsr_json.ts && bun run --shell bun publish:copy_license",
		"publish:copy_license": "cp ../../LICENSE ."
	},
	"dependencies": {
		"@rollup/pluginutils": "^5.1.0",
		"consola": "^3.2.3",
		"defu": "^6.1.4",
		"find-cache-dir": "^5.0.0",
		"magic-string": "^0.30.10",
		"pathe": "^1.1.2",
		"pkg-types": "^1.1.1",
		"ts-patch": "^3.2.1",
		"type-fest": "^4.19.0",
		"typescript": "~5.5.3",
		"typia": "^6.5.2",
		"unplugin": "^1.11.0"
	},
	"devDependencies": {
		"@antfu/ni": "^0.22.0",
		"@ryoppippi/eslint-config": "npm:@jsr/ryoppippi__eslint-config",
		"@std/assert": "npm:@jsr/std__assert",
		"@types/bun": "^1.1.6",
		"@types/node": "^20.10.3",
		"bumpp": "^9.4.1",
		"eslint": "^9.8.0",
		"eslint-plugin-format": "^0.1.2"
	}
}
